#include "Graph.h"
#include <iostream>

Graph::Graph(){}

Graph::~Graph(){}

//insert standard info into a graph
void Graph::insert(std::string key, std::string name, dist xcor, dist ycor){
  Node n = Node() ;
  n.name = name;
  n.coords = std::pair<dist,dist>(xcor,ycor);
  n.adjacent = &std::vector<Node*,dist>(); 
}

//generic insert
void Graph::insert(){
  insert("","",0,0);
}

